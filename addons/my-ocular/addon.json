{
  "name": "Show my-ocular status",
  "description": "Shows ocular statuses and favorite colours. Uses my-ocular.jeffalo.net to obtain status data. Statuses are moderated and filtered.",
  "credits": [
    {
      "name": "Jeffalo"
    }
  ],
  "userscripts": [
    {
      "url": "discuss.js",
      "matches": ["https://scratch.mit.edu/discuss/topic/*"],
      "settingMatch": {
        "id": "discuss",
        "value": true
      }
    },
    {
      "url": "profile.js",
      "matches": ["https://scratch.mit.edu/users/*/"],
      "settingMatch": {
        "id": "profile",
        "value": true
      }
    }
  ],
  "settings": [
    {
      "name": "Show on profiles",
      "id": "profile",
      "type": "boolean",
      "default": true
    },
    {
      "name": "Show on the forums",
      "id": "discuss",
      "type": "boolean",
      "default": true
    }
  ],
  "tags": ["forums", "community"],
  "enabledByDefault": false,
  "l10n": true
}
