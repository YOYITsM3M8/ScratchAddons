  "info": [
    {
      "type": "notice",
      "text": "Refresh the editor after enabling/disabling this theme.",
      "id": "refresheditor"
    }
  ],
  "credits": [
    {
      "name": "GarboMuffin"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
